    <path
      d="M3 12H21"
      stroke="white"
      stroke-width="2"
      stroke-linecap="round"
      stroke-linejoin="round"></path>
    <path
      d="M3 6H21"
      stroke="white"
      stroke-width="2"
      stroke-linecap="round"
      stroke-linejoin="round"></path>
